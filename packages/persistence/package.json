{
  "name": "@letswriteabook/persistence",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint --no-error-on-unmatched-pattern 'src/**/*.{ts,tsx}'",
    "test": "jest --config ../../jest.config.ts --passWithNoTests",
    "test:integration": "jest --config ./jest.integration.config.ts",
    "migrate": "ts-node --project tsconfig.migrations.json --esm ./scripts/migrate-cli.ts",
    "migrate:up": "pnpm run migrate up",
    "migrate:down": "pnpm run migrate down",
    "migrate:status": "pnpm run migrate status",
    "migrate:create": "pnpm run migrate create",
    "backfill:run": "ts-node --project backfill/tsconfig.json --esm ./backfill/scripts/run-backfill.ts"
  },
  "dependencies": {
    "@letswriteabook/domain": "workspace:*",
    "@letswriteabook/shared-types": "workspace:*",
    "@typegoose/typegoose": "^11.6.1",
    "mongoose": "^7.8.0",
    "migrate-mongo": "^11.0.0",
    "dotenv": "^16.4.5",
    "mongodb": "^5.9.2"
  },
  "devDependencies": {
    "mongodb-memory-server": "^10.1.4",
    "ts-node": "^10.9.2"
  }
}
